  <DataSets>
    <DataSet Name="Enrollmentandscholarshipsummary">
      <Query>
        <DataSourceName>UniDB</DataSourceName>
        <CommandText>SELECT 
    d.DepartmentName,
    p.ProgramName,
    COUNT(s.StudentID) AS TotalStudents,
    SUM(CASE WHEN s.Scholarship = 'Yes' THEN 1 ELSE 0 END) AS ScholarshipStudents,
    CASE WHEN COUNT(s.StudentID) = 0 THEN 0 
         ELSE (SUM(CASE WHEN s.Scholarship = 'Yes' THEN 1 ELSE 0 END) * 100.0) / COUNT(s.StudentID)
    END AS ScholarshipPercentage
FROM Studentss s
JOIN Program p ON s.ProgramID = p.ProgramID
JOIN Department d ON p.DepartmentID = d.DepartmentID
GROUP BY d.DepartmentName, p.ProgramName
ORDER BY d.DepartmentName, p.ProgramName;
</CommandText>
      </Query>
      <Fields>
        <Field Name="DepartmentName">
          <DataField>DepartmentName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ProgramName">
          <DataField>ProgramName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TotalStudents">
          <DataField>TotalStudents</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ScholarshipStudents">
          <DataField>ScholarshipStudents</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ScholarshipPercentage">
          <DataField>ScholarshipPercentage</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
