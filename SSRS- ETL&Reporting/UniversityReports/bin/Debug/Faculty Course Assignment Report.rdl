  <DataSets>
    <DataSet Name="FacultyCourseeAssignment">
      <Query>
        <DataSourceName>UnIversityDB</DataSourceName>
        <CommandText>SELECT 
    f.[ FacultyID],
    f.FullName AS FacultyName,
    d.DepartmentName,
    p.ProgramName,
    f.Nationality,
    s.MonthlySalary
FROM Faculty f
JOIN Department d ON f.DepartmentID = d.DepartmentID
JOIN Program p ON f.ProgramID = p.ProgramID
JOIN Salaries s ON f.[ FacultyID] = s.FacultyID
ORDER BY d.DepartmentName, f.FullName;</CommandText>
      </Query>
      <Fields>
        <Field Name="ID_FacultyID">
          <DataField> FacultyID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="FacultyName">
          <DataField>FacultyName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DepartmentName">
          <DataField>DepartmentName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ProgramName">
